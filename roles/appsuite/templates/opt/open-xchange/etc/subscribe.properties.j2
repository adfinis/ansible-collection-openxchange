{{ ansible_managed | comment }}

# Defines whether subscriptions should be run automatically on login, if a given interval has elapsed
# The intervals default to one day, but can be configured individually by subscription type
# in the subscription source specific configuration files
com.openexchange.subscribe.autorun=false

# Configures if the cross-context calendar provider for integrating federated shares is available for
# a user or not.
# Default: false
com.openexchange.calendar.xctx2.enabled={{ appsuite_calendar_xctx2_enabled | ternary('true', 'false') }}
